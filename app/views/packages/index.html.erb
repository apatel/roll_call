<div class="col-lg-12">
  <h1>Packages</h1>
</div>

<div class="col-lg-6">
  <h2>All Packages</h2>

    <div class="table-responsive">
  	<table id="libraries-table" class="table table-striped table-hover tablesorter">	
  	<thead>
  	  <tr>
  		<th>Name</th>
  		<th>Actions</th>
  	  </tr>
  	</thead> 
  	<tbody>
  	    <% @packages.each do |package| %>
  	    <tr>
  	  	  <td><%= package.name %></td>
  		  <td>
  			<%= link_to('View', package_path(package)) %>
  		  </td>
  	    </tr>
  	    <% end %>
  	</tbody>	
  	</table>
    </div>	
</div>

<div class="col-lg-6">
  <h2>All Individuals</h2>
  
  <div class="table-responsive">
	<table id="libraries-table" class="table table-striped table-hover tablesorter">	
	<thead>
	  <tr>
		<th>Name</th>
		<th>Actions</th>
	  </tr>
	</thead> 
	<tbody>
	    <% @individuals.each do |individual| %>
	    <tr>
	  	  <td><%= individual.name %></td>
		  <td>
			<%= link_to('View', individual_path(individual)) %>
		  </td>
	    </tr>
	    <% end %>
	</tbody>	
	</table>
  </div>
</div>
